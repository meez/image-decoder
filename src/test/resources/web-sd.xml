<flex-config xmlns="http://www.adobe.com/2006/flex-config">
    <target-player>20.0.0</target-player>
    <swf-version>31</swf-version>
    <default-background-color>0x000000</default-background-color>
    <default-frame-rate>24</default-frame-rate>
    <compiler>
        <define>
            <name>CONFIG::web</name>
            <value>true</value>
        </define>        
        <define>
            <name>CONFIG::air</name>
            <value>false</value>
        </define>
        <define>
            <name>CONFIG::ios</name>
            <value>false</value>
        </define>        
        <define>
            <name>CONFIG::android</name>
            <value>false</value>
        </define>
	<define>
            <name>CONFIG::Assert</name>
            <value>true</value>
        </define> 
    </compiler>
</flex-config>
